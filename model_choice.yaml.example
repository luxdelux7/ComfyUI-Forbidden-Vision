# Forbidden Vision Model Preferences Configuration
# 
# Instructions:
# 1. Rename this file to "model_choice.yaml" to activate custom preferences
# 2. Models are tried in order - first found model becomes the default
# 3. Remove or comment out models you don't want as preferred
# 4. Add your own model names to the lists
#
# Note: This file won't be overwritten by plugin updates once renamed

# Preferred models for each category
preferred_models:
  
  # Face processor upscaler models (for pre-upscaling small faces)
  upscaler:
    - "4x_NMKD-Siax_200k.pth"
    - "4x-UltraSharp.pth"
    - "4x_foolhardy_Remacri.pth"
    - "4x-AnimeSharp.pth"
  
  # Refiner node upscaler models (for final image enhancement)
  refiner_upscaler:
    - "4x-UltraSharp.pth"
    - "4x_NMKD-Siax_200k.pth"
    - "RealESRGAN_x4plus.pth"
    - "ESRGAN_4x.pth"
  
  # Primary BBOX detection models (main face detector)
  bbox_primary:
    - "face_yolov8n_v2.pt"
    - "face_yolov8m.pt"
    - "yolov8x6_animeface.pt"
    - "yolov8n-face.pt"
  
  # Secondary BBOX detection models (backup face detector)
  bbox_secondary:
    - "face_yolov8m.pt"
    - "yolov8x6_animeface.pt"
    - "face_yolov8n_v2.pt"
    - "yolov8s-face.pt"
  
  # Primary segmentation models (SAM + YOLO-seg for main masking)
  segmentation_primary:
    - "sam_vit_h_4b8939.pth"
    - "sam_vit_l_0b3195.pth"
    - "yolov8n-seg.pt"
    - "yolov8s-seg.pt"
  
  # Secondary segmentation models (YOLO-seg only for ensemble masking)
  segmentation_secondary:
    - "yolov8n-seg.pt"
    - "yolov8s-seg.pt"
    - "yolov8m-seg.pt"
    - "yolov8l-seg.pt"

# Example of how to customize for your specific models:
# 
# preferred_models:
#   upscaler:
#     - "my_favorite_face_upscaler.pth"
#   refiner_upscaler:
#     - "my_favorite_final_upscaler.pth"
#   bbox_primary:
#     - "custom_face_detector_v3.pt"
#   bbox_secondary:
#     - "backup_face_detector.pt"
#   segmentation_primary:
#     - "sam_vit_h_4b8939.pth"
#   segmentation_secondary:
#     - "custom_yolo_seg.pt"